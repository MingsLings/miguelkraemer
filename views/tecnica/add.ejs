     <%-include('../cabecalho.ejs')%>
     <header>
        <h3>Adicionar Nova Técnica</h3>
      </header>
      <div class="content">
        <form id="form-tecnica" method="POST" autocomplete="off" action="/tecnica/add/ok" >
          <div class="grid">
            <div>
              <label for="nome">Nome da Técnica</label>
              <input id="nome" name="nome" type="text" placeholder="Ex: Fala Amaldiçoada" required>
            </div>
            <div>
              <label for="usuario">Usuário(s)</label>
              <input id="usuario" name="usuario" type="text" placeholder="Ex: Clã Inumaki, Toge Inumaki" required>
            </div>
            <div>
              <label for="complexidade">Complexidade</label>
              <input id="complexidade" name="complexidade" type="text" placeholder="Ex: Simples, Mediana, Complexa" required>
            </div>
            <div>
              <label for="capacidades">Capacidades</label>
              <input id="capacidades" name="capacidades" type="text" placeholder="Ex: Suporte, Ofensiva, Defensiva" required>
            </div>
            <div>
              <label for="descricao">Descrição</label>
              <input id="descricao" name="descricao" type="text" placeholder="Ex: O usuário é capaz de comandar o alvo apenas com suas palavras." required>
            </div>
           <div class="field-help">Os campos com * são obrigatórios.</div>
          <footer>
            <a type="button" class="btn ghost" id="btn-cancelar" href="/tecnica/lst">Cancelar</a>
            <button type="submit" class="btn primary">Adicionar Técnica</button>
          </footer>
        </form>
      </div>

      <%-include('../rodape.ejs')%>